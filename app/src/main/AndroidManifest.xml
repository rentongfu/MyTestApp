<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tongfu.mytestapp">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- Include required permissions for Google Mobile Ads to run. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.BROADCAST_STICKY" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALL_LOG" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".dagger2.advanceduse2.AdvancedUseActivity2"></activity>
        <activity android:name=".dagger2.advanceduse1.AdvancedUseActivity1"></activity>
        <activity android:name=".dagger2.baseuse.DaggerBaseUseActivity" />
        <activity android:name=".databinding.DataBindingActivity"></activity>
        <activity android:name=".dagger2.Dagger2TestActivity"></activity>
        <activity android:name=".uiwidget.preferenceactivity.PreferenceFragmentTestActivity" />
        <activity android:name=".ndk.nativeactivity.MyNativeActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="mynativeactivity" />
        </activity>
        <activity android:name=".ndk.jnicallback.JniCallbackActivity" />
        <activity android:name=".ndk.hellogl2.HelloGL2Activity" />
        <activity android:name=".ndk.HelloJniActivity" />
        <activity android:name=".ndk.NdkMenuActivity" />
        <activity
            android:name=".lifecycle.activity.ActivityLifecycleDialogActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".lifecycle.fragment.FragmentLifecycleActivity" />
        <activity android:name=".uiwidget.surfaceview.SurfaceViewActivity" />
        <activity android:name=".uiwidget.UIWidgetMenuActivity" />
        <activity android:name=".uiwidget.UIWidgetActivity" />
        <activity android:name=".animation.frameanimation.FrameAnimationActivity" />
        <activity android:name=".animation.propertyanimation.PropertyAnimationActivity" />
        <activity android:name=".animation.valueanimation.ValueAnimationActivity" />
        <activity android:name=".animation.tweenanimation.TweenAnimationActivity" />
        <activity android:name=".sampleactivity.SampleMenuActivity" />
        <activity
            android:name=".sampleactivity.SampleTabActivity"
            android:label="@string/title_activity_sample_tab"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sampleactivity.SampleScrollingActivity"
            android:label="@string/title_activity_sample_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sampleactivity.SampleNavigationDrawerActivity"
            android:label="@string/title_activity_sample_navigation_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sampleactivity.SampleItemListActivity"
            android:label="@string/title_sampleitem_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".sampleactivity.SampleItemDetailActivity"
            android:label="@string/title_sampleitem_detail"
            android:parentActivityName=".sampleactivity.SampleItemListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tongfu.mytestapp.sampleactivity.SampleItemListActivity" />
        </activity>
        <activity
            android:name=".sampleactivity.ui.login.SampleLoginActivity"
            android:label="@string/title_activity_sample_login" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".sampleactivity.SampleFullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_sample_fullscreen"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".sampleactivity.SampleViewModelActivity" />
        <activity android:name=".sampleactivity.SampleEmptyActivity" />
        <activity
            android:name=".sampleactivity.SampleBottomNavigationActivity"
            android:label="@string/title_activity_sample_botton_navigation" />
        <activity
            android:name=".sampleactivity.SampleBasicActivity"
            android:label="@string/title_activity_sample_basic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".lifecycle.activity.ActivityLifecycleTestActivity2" />
        <activity android:name=".lifecycle.LifecycleActivity" />
        <activity android:name=".lifecycle.activity.ActivityLifecycleTestActivity" />
        <activity android:name=".memory.MemoryCheckActivity" />
        <activity android:name=".smoothexit.SwipeBackLayoutActivity" />
        <activity android:name=".smoothexit.SmoothExitMenuActivity" />
        <activity android:name=".permission.PermissionActivity" />

        <service
            android:name=".widget.CalendarService"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.BIND_REMOTEVIEWS" />

        <receiver android:name=".widget.MyTestWidget">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/my_test_widget_info" />
        </receiver>

        <activity android:name=".eventbus.EventBusActivity" />
        <activity android:name=".managerservice.ManagerServiceActivity" />
        <activity
            android:name=".multiprocess.MultiProcessActivity"
            android:process=":child" />
        <activity android:name=".swiperefresh.SwipeRefreshActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity" />
        <activity android:name=".crashrecord.CrashRecordActivity" />
        <activity android:name=".animation.shareelement.ShareElementActivity2" />
        <activity android:name=".animation.shareelement.ShareElementActivity1" />
        <activity android:name=".animation.AnimationEntryActivity" />
        <activity android:name=".imageload.PicassoActivity" />
        <activity android:name=".imageload.ImageLoadEntryActivity" />
        <activity
            android:name=".smoothexit.SmoothExitActivity"
            android:theme="@style/tran" />
        <activity
            android:name=".video.VideoPlayEntryActivity"
            android:configChanges="orientation" />
        <activity android:name=".video.VideoPlayActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.HOME" />
                <!-- <category android:name="android.intent.category.DEFAULT" /> -->
            </intent-filter>
        </activity>
        <activity android:name=".customview.ontouchevent1.OnTouchEventTestActivity" />
        <activity android:name=".contentprovider.ContentProviderActivity" />

        <provider
            android:name=".contentprovider.MyProvider"
            android:authorities="com.tongfu.mytestapp" />

        <service
            android:name=".service.MusicPlayService"
            android:enabled="true"
            android:exported="true"
            android:process=":music_play_service" />

        <activity android:name=".service.MusicPlayActivity" />
        <activity android:name=".broadcastreceiver.BroadcastReceiverActivity" />

        <receiver
            android:name=".broadcastreceiver.MyStaticReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".network.NetworkActivity" />
        <activity android:name=".database.DatabaseMenuActivity" />
        <activity android:name=".nonification.NotificationActivity" />
        <activity android:name=".layout.LayoutActivity" />
        <activity android:name=".menu.MenuActiivty" />
    </application>

</manifest>